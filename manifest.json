{
    "manifest_version": 3,
    "name": "Multi Keybind Site Opener",
    "version": "1.0",
    "description": "Assign multiple keybinds to open multiple sites.",
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "open-site-1": {
            "suggested_key": {
                "default": "Ctrl+Shift+1"
            },
            "description": "Open site assigned to slot 1"
        },
        "open-site-2": {
            "suggested_key": {
                "default": "Ctrl+Shift+2"
            },
            "description": "Open site assigned to slot 2"
        },
        "open-site-3": {
            "suggested_key": {
                "default": "Ctrl+Shift+3"
            },
            "description": "Open site assigned to slot 3"
        }
    }
}